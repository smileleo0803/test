--105µ§¦W³æBase
select * 
from temp_analysisdev.Z

/*¥x¥~¹ô¶}¤á*/
/*AUM¾lÃB*/
/*SB Loan¾lÃB*/
/*¥~¹ô¾lÃB*/
/*IPAD¬ù³X±M®a(¦ê±M®a¹Î¶¤³­³X)*/
left join
(
select a.FA_Event_Nbr
        , max(case when c.team='FP' then c.employee_name else '' end) as "FP"
        , max(case when c.team='PA' then c.employee_name else '' end) as "PA"
        , max(case when c.team='IP' then c.employee_name else '' end) as "IP"
        , max(case when c.team='IS' then c.employee_name else '' end) as "IS"
        , max(case when c.team='FPIµ|' then c.employee_name else '' end) as "FPI-µ|"
        , max(case when c.team='FPI¤£' then c.employee_name else '' end) as "FPI-¦a"
        , max(case when c.team='FPI«H' then c.employee_name else '' end) as "FPI-«H"
    --±M®a³­³X¦æµ{³øªíÀÉ-
from
(
sel FA_Event_Nbr
     , Expert_Employee_Nbr
from vr_bns_mcif.INWM_EXPERT_EVENT
where FA_visit_start_date/100=11902  --§ï
    and cancel_code is null --1¬O¤w§R°£  ¨ä¥L³£ªÅ­È ¬G¨úNull
    and approval_code=1 --¥DºÞ¼f®Öª¬ºA 1.¤w³q¹L
group by 1,2
) as a
)
/*§¹¦¨Call Report¶ñ¼g*/
sel    unique customer_id
from VP_MF_MCIF.FPS_VISIT_RECORD
where VISIT_DATE between 1190101 and 1190930

